%This is a list of constraints to translate first.
Not(negint) || \CC\backslash\ZZ^-
Not(nonposint) || \CC\backslash\ZZ_0^-
Not(posint) || \CC\backslash\ZZ^+
::Not(positive) || < 0
Not(integer) || \CC\backslash\ZZ
posint || \mathbb{Z^+}
nonnegint || \ZZ\backslash\ZZ^-
::positive || > 0
::nonnegative || \geq 0
::nonneg || \geq 0